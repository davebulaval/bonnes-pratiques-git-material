
\mode<presentation>


\definecolor{ulred}{RGB}{255,107,107}
\definecolor{ulgold}{RGB}{255,230,109}
\definecolor{ulgray}{RGB}{204,204,204}
\definecolor{ulblue}{RGB}{43,252,226}
\definecolor{ulbeige}{RGB}{252,252,245}


%\setbeamercolor{structure}{fg=ulblue!40!black}
\setbeamercolor{structure}{fg=ulred}

\setbeamercolor{palette primary}{fg=black,bg=ulblue}
\setbeamercolor{palette secondary}{fg=black,bg=ulblue}
\setbeamercolor{palette tertiary}{fg=white,bg=ulred}
\setbeamercolor{palette quaternary}{fg=white,bg=ulred}

\setbeamercolor{titlelike}{fg=ulred!80!black}
%\setbeamercolor{frametitle}{bg=ulbeige!75}

\setbeamercolor{block title}{fg=black,bg=ulgold}
\setbeamercolor{block title alerted}{use=alerted text,fg=black,bg=alerted text.fg!75!bg}
\setbeamercolor{block title example}{use=example text,fg=black,bg=example text.fg!75!bg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}


\setbeamercolor*{separation line}{bg=ulred}
\setbeamercolor*{fine separation line}{}

\mode
<all>
